{
    "dataspin": {
        "working_dir": "./tmp/working"
    },
    "streams": [
        {
            "name": "local_simple_data",
            "data_format": "dataspin",
            "url": "watch+local://./tmp/source"
        }
    ],
    "storages": [
        {
            "name": "simple_target",
            "url": "file://./tmp/target/"
        }
    ],
    "data_views": [
        {
            "name": "table",
            "table_format": "jsonl",
            "fields": [
                {
                    "name": "app_id",
                    "type": "string"
                },
                {
                    "name": "event_name",
                    "type": "string"
                },
                {
                    "name": "event_id",
                    "type": "string"
                },
                {
                    "name": "bp_timestamp",
                    "type": "date"
                }
            ]
        }
    ],
    "data_processes": [
        {
            "name": "split and build index",
            "description": "",
            "source": "local_simple_data",
            "processes": [
                {
                    "name": "format by data_view",
                    "function": "format",
                    "args": {
                        "table_name": "table"
                    }
                },
                {
                    "name": "filter",
                    "function": "filter",
                    "args": {
                        "filter_rules": [
                            {
                                "tags": {"filter": "specific"},
                                "rule": "app_id in ['APPIOXDKXIESP','APPOWLSLSDWLD']"
                            },
                            {
                                "tags": {"filter": "default"},
                                "rule": "True"
                            }
                        ]
                    }
                },
                {
                    "name": "split by app_id",
                    "function": "splitby",
                    "args": {
                        "key": ["app_id", "event_name"],
                        "tags": {"app_id":"{data.app_id}","service":"dataspin", "event_name":"{data.event_name}"}
                    }
                },
                {
                    "name": "build primary key index",
                    "function": "pk_index",
                    "args": {
                        "key": [
                            "app_id",
                            "event_id"
                        ]
                    }
                },
                {
                    "name": "flatten json",
                    "function": "flatten"
                },
                {
                    "name": "deduplicate by event_id",
                    "function": "deduplicate",
                    "args": {
                        "key": [
                            "app_id",
                            "event_id"
                        ]
                    }
                },
                {
                    "name": "merge file by tag",
                    "function": "merge",
                    "args": {
                        "tags": ["event_name"],
                        "output_file_lines": 5000
                    }
                },
                {
                    "name": "save to target",
                    "function": "save",
                    "args": {
                        "location": "simple_target",
                        "path_suffix": "{service}/filter={filter}/app_id={app_id}",
                        "table_name": "table"
                    }
                }
            ]
        }
    ]
}